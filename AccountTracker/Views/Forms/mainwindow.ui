   <string notr="true">/* Общий стиль для окна */
QMainWindow {
    background-color: #2c2f33;
    color: #000;
    font-family: Arial, sans-serif;
    font-size: 14px;
}

/* QGraphicsView стиль */
QGraphicsView {
    border: 2px solid #7289da;
    border-radius: 5px;
    background-color: #23272a;
}

/* QComboBox стиль */
QComboBox {
    border: 1px solid #99aab5;
    border-radius: 5px;
    padding: 5px;
    background-color: #23272a;
    color: #ffffff;
}
QComboBox::drop-down {
    border: none;
    background: #7289da;
    width: 20px;
}
QComboBox::down-arrow {
    image: url(:/icons/arrow-down.png); /* При необходимости замените иконку */
    width: 10px;
    height: 10px;
}

/* QPushButton стиль */
QPushButton {
    background-color: #7289da;
    border: 1px solid #7289da;
    border-radius: 5px;
    padding: 5px;
    color: #ffffff;
}
QPushButton:hover {
    background-color: #99aab5;
    border: 1px solid #99aab5;
}
QPushButton:pressed {
    background-color: #23272a;
    border: 1px solid #99aab5;
}

/* QSpacerItem стиль (используется для регулирования пространства) */
QSpacerItem {
    background: transparent;
}

/* ScrollBars (если они есть внутри QGraphicsView) */
QScrollBar:vertical {
    background: #2c2f33;
    border: none;
    width: 10px;
}
QScrollBar::handle:vertical {
    background: #7289da;
    border-radius: 5px;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    background: none;
}
QScrollBar:horizontal {
    background: #2c2f33;
    border: none;
    height: 10px;
}
QScrollBar::handle:horizontal {
    background: #7289da;
    border-radius: 5px;
}
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    background: none;
}

/* QGroupBox стиль */
QGroupBox {
    background-color: #2c2f33;
    color: #fff;
	font-size: 18px;
    border: 1px solid #7289da;
    border-radius: 5px;
    margin-top: 10px;
    margin-bottom: 10px;
}

QLabel {
	color: #000;
	font-size: 16px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 5px;
    padding: 0 3px;
}
</string>
